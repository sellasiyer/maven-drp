        <xs:schema xmlns:dt="http://www.tsh.bestbuy.com/common/datatype/v1" xmlns:tns="http://tsh.bestbuy.com/tsh/tshfault" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:cf="http://www.tsh.bestbuy.com/common/v1" targetNamespace="http://tsh.bestbuy.com/tsh/tshfault" elementFormDefault="qualified" attributeFormDefault="unqualified">
            <xs:import namespace="http://www.tsh.bestbuy.com/common/datatype/v1"/>
